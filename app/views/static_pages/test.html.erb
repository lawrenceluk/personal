<% content_for :title, "Top Secret Best Game Ever" %>
<div class="row">
	<div class="col-sm-6 col-sm-push-3 unselectable">
		<h2 class="center">Best Game Ever</h2>
		<div class="canvas_zone">
			<canvas id="canvas" width= "960" height="800"></canvas>
		</div>
		<p class="center">Click to go up. Don't crash into anything. Don't run out of gas. Land on the ground to refuel.</p>
	</div>
</div>

<% content_for :javascript_includes do %>
  <%= javascript_include_tag "easeljs.js" %>
  <%= javascript_include_tag "simplegame.js" %>
<% end %>